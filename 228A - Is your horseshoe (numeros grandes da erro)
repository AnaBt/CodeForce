#include <iostream>
#include <string>
using namespace std;
 
int main()
{
    int cor1,cor2,cor3,cor4;
    int iguais = 0;
    int lig = 0;
 
    cin >> cor1 >> cor2 >> cor3 >> cor4;
    int array[4] = {cor1,cor2,cor3,cor4};
    for( int i = 0 ; i < 4 ; i++){
        if (i + 1 >= 4){
            break;
        }
        if(array[i] == array[i+1]){
            lig ++;
        }
    }
    
    cout << lig;
    

    return 0;
}
